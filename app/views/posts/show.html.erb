<h1><%= @post.title %></h1>
<h2><%= @post.content %></h2>

<h3>Categories:</h3>
<% @post.categories.each do |category| %>
  <p><%= category.name %></p>
<% end %>


<h3>Comments:</h3>
<% @post.comments.each do |comment| %>
  <p><%= comment.content %></p>
<% end %>

<h3>Unique Users:</h3>

<% @post.users.uniq.each do |user| %>
  <p><%= link_to user.username, user_path(user)%></p>
<% end %>
